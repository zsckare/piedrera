<div class="row">
	<div class="row">
		<h4 class="center-align">Ultimas Liquidaciones</h4>
	</div>
	<div class="row">
  <div class="col s12 m12 l12">

		<table>
		  <thead>
		    <tr>
		      <th>Camion</th>
		      
		      <th>Turno</th>
		      <th>Efectivo</th>
		      <th>Gastos</th>
		      <th>Terreno</th>
		      <th>Ahorro</th>
		      <th>Otro</th>
		      <th colspan="3"></th>
		    </tr>
		  </thead>

		  <tbody>
		    <% @liquidations.each do |liquidation| %>
		      <tr>
		        <td><%= liquidation.camion %></td>
		        
		        <td><%= liquidation.turno %></td>
		        <td><%= liquidation.efectivo %></td>
		        <td><%= liquidation.gastos %></td>
		        <td><%= liquidation.terreno %></td>
		        <td><%= liquidation.ahorro %></td>
		        <td><%= liquidation.otro %></td>
		        <td class="right" >
		          <%= link_to 'Ver', liquidation, class: 'btn blue white-text' %>
		          </td>
		      </tr>
		    <% end %>
		  </tbody>
		</table>

  </div>
</div>
		
</div>