<div class="row">
  <div class="col s12 m12 l12">

  <div class="row">
    <h4 class="center-align">Lista de Titulares</h4>
  </div>

  <div class="row">    
    <table>
      <thead>
        <tr>
          <th>Nombre</th>
          <th>Paterno</th>
          <th>Materno</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @titulars.each do |titular| %>
          <tr>
            <td><%= titular.nombre %></td>
            <td><%= titular.paterno %></td>
            <td><%= titular.materno %></td>
            <td class="right">
            <%= link_to 'Ver', titular, class: 'btn blue' %>
            <%= link_to 'Editar', edit_titular_path(titular), class: 'btn ' %>
            <%= link_to 'Eliminar', titular, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn red' %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

  </div>

  <div class="row">
    <%= link_to 'Nuevo Titular', new_titular_path, class: 'btn blue' %>    
  </div>


  </div>
</div>